<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="combo_produto_selecionado">
        <item>produto.descricao</item>
    </string-array>

    <string-array name="combo_produto_filtro">
        <item>produto.descricao</item>
        <item>produto.codigo</item>
    </string-array>


    <!-- Campos / Array da tela para listagem das vendas -->
    <array name="ItemVendaListagemID">
        <item><string>Venda.ID</string></item>
        <item>@id/edtVendaListagemID</item>
    </array>
    <array name="LayoutVendaListagem">
        <item>@array/ItemVendaListagemID</item>
    </array>

    <declare-styleable name="CustomLayoutLP">
        <attr name="android:layout_gravity" />
        <attr name="layout_position">
            <enum name="middle" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

</resources>